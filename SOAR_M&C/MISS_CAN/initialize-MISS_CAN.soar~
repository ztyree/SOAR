sp {propose*initialize-MISS_CAN
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-MISS_CAN)
}

sp {apply*initialize-MISS_CAN
   (state <s> ^operator <op>)
   (<op> ^name initialize-MISS_CAN)
-->
   (<s> ^name MISS_CAN)
}

sp {apply*initialize-MISS_CAN
   (state <s> ^operator.name initialize-MISS_CAN)
-->
   (<s> ^name MISS_CAN
        ^leftBank <lb>
        ^rightBank <rB>
        ^desired <d>)
   (<lb> ^missionaries 3
         ^cannibals 3
         ^boat yes
         ^otherBank <rb>)
   (<rb> ^missionaries 0
        ^cannibals 0
        ^boat no
        ^otherBank <lb>)
   (<d> ^rightBank <dr>)
   (<dr> ^missionaries 3
         ^cannibals 3
         ^boat yes)}